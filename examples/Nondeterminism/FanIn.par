// Try in the Playground!

def TryFanIn = FanIn(type String)

dec FanIn : [type a] [List<List<a>>] List<a>
def FanIn = [type a] [lists] chan yield {
  let yield = Cell.Share(yield, chan cell {
    lists.begin.case {
      .end! => {
        cell.end!
      }

      .item(list) => {
        cell.split(chan cell {
          list.begin.case {
            .end! => {
              cell.end!
            }

            .item(value) => {
              cell.take[yield]
              yield.item(value)
              cell.put(yield)
              list.loop
            }
          }
        })

        lists.loop
      }
    }
  })

  yield.end!
}
