def Bool.Not: [Bool] Bool = [x] x.case {
    .true! => .false!,
    .false! => .true!,
}

def TestAssertTrue: [Test] ! = [test] chan user {
    test.assert("Hi", .true!)
    test.done <> user
}

def TestBoolNot: [Test] ! = [test] chan user {
    test.assert("Hi", Bool.Not(.false!))
    test.done <> user
}
